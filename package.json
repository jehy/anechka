    "googleapis": "65.0.0",
    "moment": "2.29.1",
    "slack": "11.0.2",
    "stringify-clone": "2.0.3"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "debug": "4.3.1",
    "deep-freeze": "0.0.1",
